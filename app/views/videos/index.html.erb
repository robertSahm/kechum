<h1>Listing videos</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Brand</th>
    <th>Type of</th>
    <th>Director</th>
    <th>Url</th>
    <th>Description</th>
    <th>Permalink</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%= video.project %></td>
    <td><%= video.brand %></td>
    <td><%= video.type_of %></td>
    <td><%= video.director %></td>
    <td><%= video.url %></td>
    <td><%= video.description %></td>
    <td><%= video.permalink %></td>
    <td><%= link_to 'Show', video %></td>
    <td><%= link_to 'Edit', edit_video_path(video) %></td>
    <td><%= link_to 'Destroy', video, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Video', new_video_path %>
